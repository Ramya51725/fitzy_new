{
    "version": 2,
    "builds": [
        {
            "src": "Fitzy-changes-backend/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "Fitzy-changes-backend/main.py"
        },
        {
            "src": "/(config.js|index.html|fitzy_frontend/.*)",
            "dest": "/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}