{
    "version": 2,
    "builds": [
        {
            "src": "backend/main.py",
            "use": "@vercel/python"
        },
        {
            "src": "index.html",
            "use": "@vercel/static"
        },
        {
            "src": "fitzy_frontend/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/backend/(.*)",
            "dest": "backend/main.py"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}