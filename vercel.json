{
    "version": 2,
    "builds": [
        {
            "src": "backend/main.py",
            "use": "@vercel/python"
        },
        {
            "src": "index.html",
            "use": "@vercel/static"
        },
        {
            "src": "fitzy_frontend/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/fitzy_frontend/(.*)",
            "dest": "/fitzy_frontend/$1"
        },
        {
            "src": "/index.html",
            "dest": "/index.html"
        },
        {
            "src": "/((?!fitzy_frontend|index\\.html).*)",
            "dest": "backend/main.py"
        }
    ]
}