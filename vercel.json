{
    "version": 2,
    "builds": [
        {
            "src": "backend/main.py",
            "use": "@vercel/python"
        },
        {
            "src": "index.html",
            "use": "@vercel/static"
        },
        {
            "src": "fitzy_frontend/**",
            "use": "@vercel/static"
        }
    ],
    "rewrites": [
        {
            "source": "/(users|exercise-progress|diet-progress|exercise|category|veg|nonveg|exercise-log|health)(/.*)?",
            "destination": "backend/main.py"
        }
    ]
}